        <div class="container">
            <div id="nav-toggle" class="nav-toggle">
                <span></span>
            </div>
            <span id="main-logo" class="logo">
                <em>H</em>elper <em>C</em>linic
            </span>
            <nav role="navigation">
                <ul>
                    <li class="">
                        <a href="#home" data-hover="Home">Home</a>
                    </li>
                    <li class="">
                        <a href="#services" data-hover="Serviços">Serviços</a>
                    </li>
                    <li class="">
                        <a href="#portfolio" data-hover="Compatibilidade">Compatibilidade</a>
                    </li>
                    <li class="">
                        <a href="#skills" data-hover="Preços">Preços</a>
